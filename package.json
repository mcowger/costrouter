{
  "name": "llm-gateway-root",
  "version": "1.0.0",
  "private": true,
  "description": "An intelligent gateway for routing requests to LLM providers.",
  "main": "dist/server/index.js",
  "type": "module",
  "imports": {
    "#types/*": "./dist/types/*.js",
    "#server/*": "./dist/server/*.js"
  },
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "build": "tsc --build",
    "build:types": "tsc --build types",
    "build:server": "tsc --build server",
    "dev": "concurrently \"npm:dev:server\"",
    "dev:server": "nodemon --exec node --loader ts-node/esm server/src/index.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.53",
    "@ai-sdk/deepinfra": "^1.0.29",
    "@ai-sdk/fireworks": "^1.0.29",
    "@ai-sdk/google": "^2.0.44",
    "@ai-sdk/openai": "^2.0.77",
    "@ai-sdk/togetherai": "^1.0.29",
    "@ai-sdk/xai": "^2.0.39",
    "@openrouter/ai-sdk-provider": "^1.4.0",
    "ai": "^5.0.106",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "openai": "^6.9.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "yargs": "^18.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@types/yargs": "^17.0.35",
    "concurrently": "^9.2.1",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}