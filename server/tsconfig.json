{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist/server",
    "rootDir": "src",
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "tsBuildInfoFile": "../dist/server.tsbuildinfo",
    "paths": {
      "#types/*": ["../types/src/*"]
    }
  },
  "include": ["src/**/*"],
  "references": [
    { "path": "../types" }
  ]
}